<h2>Edit Historical Landmark</h2>

<form action="/landmarks/<%= @landmark.id %>" method="post">
    <input id="hidden" type="hidden" name="_method" value="patch">
    <% @landmark.each do |landmark| %>
        <input type="text" id="name" name="landmark[name] value="<%= landmark.name %>">
        <input type="text" id="year_completed" name="landmark[year_completed] value="<%= landmark.year_completed.to_s %>">
    <% end %>

    <input type="button" value="Edit Landmark">

</form>

    <a class="button" href="/landmarks">Landmarks</a>
    <a class="button" href="/">Home</a>


